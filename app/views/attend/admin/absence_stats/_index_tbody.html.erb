<tr>
  <td><%= absence_stat.id %></td>
  <td><%= absence_stat.member.name %></td>
  <td><%= absence_stat.annual_days %></td>
  <td><%= absence_stat.year %></td>
  <td>
    <%#= link_to admin_absence_stat_path(absence_stat), aria: { label: t('.show') }, class: 'ui blue mini icon button' do %>
<!--          <i class="fas fa-eye"></i>-->
    <%# end %>
    <%= link_to edit_admin_absence_stat_path(absence_stat), aria: { label: t('.edit') }, class: 'ui pink mini icon button' do %>
      <i class="fas fa-pencil-alt"></i>
    <% end %>
    <%= link_to admin_absence_stat_path(absence_stat), method: :delete, aria: { label: t('.destroy') }, data: { confirm: t('.confirm') }, class: 'ui red mini icon button' do %>
      <i class="fas fa-trash"></i>
    <% end %>
  </td>
</tr>
